/* ==================
        初始化
 ==================== */
page {
	background: #fff;
	font-size: 30rpx;
	color: #333333;
	/* font-family: '微软雅黑'; */
	font-family: Arial,Helvetica,sans-serif;
	/* overflow:hidden; */
}
view,
scroll-view,
swiper,
button,
input,
textarea,
label,
navigator,
._table,
._tr,
._td,
image {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.none {
	display: none;
}
.round {
	-webkit-border-radius: 5000rpx;
	        border-radius: 5000rpx;
}
.radius {
	-webkit-border-radius: 6rpx;
	        border-radius: 6rpx;
}
button::after {
	border: none;
}
.icon {
	width: 40rpx;
	height: 40rpx;
	padding: 0;
}
.icon image {
	width: 40rpx;
	height: 40rpx;
}
.underline{
	 text-decoration: underline;
}
.button-active{
	opacity: 0.8;
}
image {
	max-width: 100%;
	display: inline-block;
	position: relative;
	z-index: 0;
}
.clearfix:after, .clearfix:before{
	display: table;
	content: ""
}
.clearfix:after {
	clear: both
}
.response {
	width: 100%;
}
.placeholder-class{
	color: #ccc;
	font-size:28rpx;
}
checkbox::before {  
	position: absolute;
	color: #ffffff !important;
	top: 50%;
	margin-top: -8px;
	right: 5px;
	font-size: 32rpx;
	line-height: 16px;
	pointer-events: none;
	-webkit-transform: scale(1, 1);
	        transform: scale(1, 1);
	-webkit-transition: all 0.3s ease-in-out 0s;
	transition: all 0.3s ease-in-out 0s;
	z-index: 9;
}
checkbox .wx-checkbox-input, 
checkbox .uni-checkbox-input {
	margin: 0;
	width: 24px;
	height: 24px;
	-webkit-border-radius: 50%;
	        border-radius: 50%;  
	-webkit-transform: scale(0.6);  
	        transform: scale(0.6);
	border: 0;
	background: url(data:image/png;base64,) no-repeat;
	background-size: cover ;
}
checkbox .wx-checkbox-input::before, 
checkbox .uni-checkbox-input::before 
  {
	display: none;
}
checkbox[checked] .wx-checkbox-input, 
checkbox.checked .uni-checkbox-input, 
.uni-checkbox-input.uni-checkbox-input-checked { 
	background: url(data:image/png;base64,) no-repeat;
	background-size: cover ;
}
uni-radio .uni-radio-input{
	width: 24rpx;
    height: 24rpx;
}
.btn{
	text-align: center;
	color:#FFF;
	height:100rpx;
	line-height: 100rpx;
	background:rgba(255,144,37,1);
	border-radius:50rpx;
	-webkit-border-radius:50rpx;
	-moz-border-radius:50rpx;
	-ms-border-radius:50rpx;
	-o-border-radius:50rpx;
	font-size: 36rpx;
}

/*倒三角*/
.corner {
	width: 0;
	height: 0;
	border: 10rpx solid #FF9025;
	border-bottom-color: transparent;   
	border-left-color: transparent;
	border-right-color: transparent;
}
.iconicon-endurance{
	color: #FFA800;	
	margin-right: 6rpx;
}
.icontanhao{
	color: #655553;
	margin-left: 8rpx; 
	vertical-align: text-top;
	font-size: 30rpx;
}

