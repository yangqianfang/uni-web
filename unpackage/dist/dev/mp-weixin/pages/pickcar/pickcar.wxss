.topbar {
  position: fixed;
  color: #716261;
  z-index: 2;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 30rpx 30rpx;
  width: 100%;
  line-height: 40rpx;
}
.status {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.user-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.iconren {
  color: #333333;
  font-size: 40rpx;
}
.status-text {
  margin-left: 15rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #000;
}
.dot-icon {
  width: 10rpx;
  height: 10rpx;
  background: #ff4252;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  position: absolute;
  right: 0;
  top: -5rpx;
}
.user-text {
  overflow: hidden;
}
.user-text-address {
  margin-left: 10rpx;
  font-size: 30rpx;
  padding-right: 9rpx;
  height: 34rpx;
  line-height: 50rpx;
  float: left;
}
.iconxiangxiayuan {
  color: #655553;
  line-height: 50rpx;
}
.kefu-icon {
  width: 38rpx;
  height: 42rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}
.cancel-order {
  font-size: 30rpx;
  color: #000;
}
.wrap {
  width: 100%;
  height: 100vh;
  position: relative;
  overflow: hidden;
}
.map {
  position: fixed;
  z-index: 1;
  height: 100vh;
  /* 	height: 100vh; */
  width: 100%;
  background: #eee;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  /*垂直居中*/
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  /*水平居中*/
}
.bottom-tips {
  position: fixed;
  bottom: 30rpx;
  width: 100%;
  z-index: 3;
}
.bottom-tips .bottom-text {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
  color: #FF9025;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  background: #fff;
  width: 75%;
  margin: 0 auto;
}
.line-up-layer {
  position: fixed;
  bottom: 100rpx;
  width: 100%;
  z-index: 3;
}
.line-up-layer .lup-wrap {
  color: #666;
  font-size: 30rpx;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  background: #eee;
  width: 90%;
  margin: 0 auto;
  padding: 30rpx;
  text-align: center;
}
.line-up-layer .lup-wrap .lup-title {
  padding-bottom: 30rpx;
}
.line-up-layer .lup-wrap .lup-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.line-up-layer .lup-wrap .lup-content .lup-item {
  width: 50%;
  text-align: center;
}
.line-up-layer .lup-wrap .lup-content .lup-item .lup-text {
  display: block;
  padding-bottom: 10rpx;
}
.line-up-layer .lup-wrap .lup-content .lup-item .lup-text .lup-num {
  font-size: 40rpx;
  color: #FF9025;
}

