.topbar {
  position: fixed;
  color: #716261;
  z-index: 2;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 30rpx 30rpx;
  width: 100%;
  line-height: 40rpx;
}
.status {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.user-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.iconren {
  color: #333333;
  font-size: 40rpx;
}
.status-text {
  margin-left: 15rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #000;
}
.dot-icon {
  width: 10rpx;
  height: 10rpx;
  background: #ff4252;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  position: absolute;
  right: 0;
  top: -5rpx;
}
.user-text {
  overflow: hidden;
}
.user-text-address {
  margin-left: 10rpx;
  font-size: 30rpx;
  padding-right: 9rpx;
  height: 34rpx;
  line-height: 50rpx;
  float: left;
}
.iconxiangxiayuan {
  color: #655553;
  line-height: 50rpx;
}
.kefu-icon {
  width: 38rpx;
  height: 42rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}
.cancel-order {
  font-size: 30rpx;
  color: #000;
}
.wrap {
  padding: 0rpx;
}
.wrap .item {
  border-bottom: 1px solid #E1E1E1;
  padding: 30rpx;
}
.wrap .item .conten-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.wrap .item .conten-wrap .image-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-flow: column;
          flex-flow: column;
}
.wrap .item .conten-wrap .image-wrap .imagebox {
  width: 202rpx;
  height: 132rpx;
}
.wrap .item .conten-wrap .image-wrap .imagebox image {
  width: 100%;
  height: 100%;
}
.wrap .item .conten-wrap .image-wrap .text {
  text-align: center;
}
.wrap .item .conten-wrap .info-wrap {
  padding-left: 20rpx;
}
.wrap .item .conten-wrap .info-wrap .li {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 8rpx 0;
  line-height: 40rpx;
}
.wrap .item .conten-wrap .info-wrap .li .label {
  padding-right: 20rpx;
  color: #696969;
  width: 200rpx;
}
.wrap .item .conten-wrap .info-wrap .li .label-text {
  width: 100%;
}
.wrap .button-wrap {
  padding: 10rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  text-align: right;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
}
.wrap .button-wrap button {
  margin: 0 10rpx;
  padding: 10rpx 20rpx;
  font-size: 24rpx;
  line-height: 30rpx;
}
.wrap .button-wrap .cancel-order {
  border: 1px solid #999;
  background: #fff;
}
.wrap .button-wrap .pay-order {
  background: #ff9025;
  color: #fff;
  border: 1px solid #ff9025;
}
.no-data {
  text-align: center;
  padding: 30rpx;
}

