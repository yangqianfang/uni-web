<view class="wrap"><block wx:if="{{nodata}}"><view class="no-data">没有预约订单</view></block><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view class="conten-wrap"><view class="image-wrap"><view class="imagebox"><image src="{{item.$orig.car_image}}"></image></view><text class="text">{{item.$orig.car_plate}}</text></view><view class="info-wrap"><view class="li"><text class="label">用车模式</text><text class="label-text">{{item.$orig.use_time_type==0?'时租':'日租'}}</text></view><view class="li"><text class="label">用车位置</text><text class="label-text">{{item.$orig.use_place+''}}</text></view><view class="li"><text class="label">下单时间</text><text class="label-text">{{item.f0}}</text></view><view class="li"><text class="label">用车时间</text><text class="label-text">{{item.f1+''}}</text></view><view class="li"><text class="label">费用</text><text class="label-text">{{item.$orig.power_on_price+"元/分钟(行驶) "+item.$orig.power_off_price+'元/分钟(临停)'}}</text></view></view></view><view class="button-wrap"><button data-event-opts="{{[['tap',[['checkCancel',['$0'],[[['orderList','',index]]]]]]]}}" class="cancel-order" bindtap="__e">取消订单</button><block wx:if="{{item.$orig.use_time_type!=0}}"><button data-event-opts="{{[['tap',[['gotoPay',['$0'],[[['orderList','',index,'pay_id']]]]]]]}}" class="pay-order" bindtap="__e">去支付</button></block></view></view></block></view>