.cy-body {
  width: 560rpx;
  padding: 50rpx 36rpx 50rpx 36rpx;
  background: #fff;
  position: relative;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  overflow: visible;
  text-align: center;
  z-index: 99999;
}
.cy-body .title {
  font-size: 36rpx;
  text-align: center;
  font-weight: bold;
  margin-bottom: 30rpx;
}
.cy-body .order-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  color: #666;
  font-size: 28rpx;
  padding: 15rpx 0;
}
.cy-body .order-item .price-type {
  color: #FF9025;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.cy-body .detailbox {
  margin: 30rpx auto;
  width: 320rpx;
  height: 88rpx;
  font-size: 24rpx;
  line-height: 88rpx;
  -webkit-border-radius: 2px;
          border-radius: 2px;
  color: #666;
  border: 1px solid #e8e8e8;
}
.cy-body .buttonbox {
  padding-top: 30rpx;
}
.cy-body .buttonbox button {
  color: #fff;
  width: 208px;
  font-size: 36rpx;
  line-height: 50px;
  height: 50px;
  background: #ff9025;
  -webkit-box-shadow: 0px 2px 8px 0px rgba(255, 144, 37, 0.59);
          box-shadow: 0px 2px 8px 0px rgba(255, 144, 37, 0.59);
  -webkit-border-radius: 25px;
          border-radius: 25px;
}
.cy-body .close {
  width: 42rpx;
  height: 42rpx;
  border: 0;
  position: absolute;
  left: 50%;
  bottom: -100rpx;
  z-index: 999999;
  margin-left: -21rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  padding: 0;
}
 .uni-popup {
  overflow: visible;
}
 .uni-popup__wrapper.uni-custom.center .uni-popup__wrapper-box {
  overflow: visible;
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  padding: 0;
}
checkbox-group {
  text-align: right;
}

