<view class="container"><view class="container-header"><view class="search-header"><view class="search-container"><text class="iconfont iconsousuo"></text><input class="uni-input" placeholder="请输入位置" data-event-opts="{{[['input',[['__set_model',['','key','$event',[]]],['onSearch',['$event']]]]]}}" value="{{key}}" bindinput="__e"/></view><text data-event-opts="{{[['tap',[['onCancel',['$event']]]]]}}" class="btn-cancel" bindtap="__e">取消</text></view></view><view class="search-result" style="{{'height:'+(listHeight)+';'}}"><block wx:for="{{mapList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['onChoose',['$0','$1'],[[['mapList','',index,'location']],[['mapList','',index,'name']]]]]]]}}" class="search-item" bindtap="__e"><text class="iconfont iconweizhi"></text><view class="search-block item-content"><view class="address-name">{{item.name}}<text>附近5处还车网点</text></view><text class="address-desc">{{item.district+" 距离当前位置12km"}}</text></view><view class="search-block item-opt"><text class="iconfont icon-iconbianzubeifenx3"></text></view></view></block></view><block wx:if="{{mapList.length>0}}"><view data-event-opts="{{[['tap',[['clearHistory',['$event']]]]]}}" class="clear-btn" bindtap="__e">清空历史记录</view></block></view>